     <string notr="true">QTabWidget::pane {
    border: none; /* Loại bỏ viền của QTabWidget */
    background-color: transparent; /* Đặt nền trong suốt cho pane */
}

QTabBar::tab {
    background-color: #1a3c6d; /* Màu nền của các tab (xanh biển đậm) */
    color: white; /* Màu chữ trắng */
    border: 2px solid #1a3c6d; /* Viền của tab (xanh biển đậm) */
    border-radius: 0px; /* Không bo góc giữa các tab */
    padding: 10px; /* Khoảng cách giữa văn bản và viền */
    margin-right: 0px; /* Loại bỏ khoảng cách bên phải giữa các tab */
}

QTabBar::tab:first {
    border-top-left-radius: 10px; /* Bo tròn góc trái trên của tab đầu tiên */
    border-top-right-radius: 0px; /* Không bo tròn góc phải trên của tab đầu tiên */
}

QTabBar::tab:last {
    border-top-right-radius: 10px; /* Bo tròn góc phải trên của tab cuối cùng */
    border-top-left-radius: 0px; /* Không bo tròn góc trái trên của tab cuối cùng */
}

QTabBar::tab:selected {
    background-color: #4a90e2; /* Màu nền của tab khi chọn (xanh biển nhạt hơn) */
    color: white; /* Màu chữ khi chọn (trắng) */
    border-color: #4a90e2; /* Viền của tab khi chọn (xanh biển nhạt hơn) */
}

QTabBar::tab:hover {
    background-color: #76d7c4; /* Màu nền của tab khi hover (xanh lá nhạt hơn) */
    border-color: #76d7c4; /* Viền của tab khi hover (xanh lá nhạt hơn) */
}

/* Căn giữa QTabWidget */
QTabWidget {
    background-color: #ecf0f1; /* Màu nền của QTabWidget */
    border-radius: 10px; /* Bo tròn các góc của QTabWidget */
    padding: 5px;
    margin: 0 auto; /* Căn giữa QTabWidget */
    display: block; /* Đảm bảo QTabWidget được hiển thị như một block */
}
