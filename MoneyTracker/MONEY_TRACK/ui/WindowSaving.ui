    <property name="styleSheet">
     <string notr="true">/* Tùy chỉnh cho QTabWidget */
QTabWidget::pane {
    border: none; /* Xóa viền của QTabWidget */
    background-color: transparent; /* Nền trong suốt */
}

QTabBar::tab {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #4e79b0, stop:1 #1e5fa1); /* Gradient nền của tab */
    color: white; /* Màu chữ */
    border: 2px solid #1e5fa1; /* Viền tab */
    border-radius: 12px; /* Bo tròn góc của tab */
    padding: 12px; /* Khoảng cách giữa văn bản và viền */
    margin-right: 6px; /* Khoảng cách giữa các tab */
    font-size: 14px; /* Kích thước font chữ */
    font-weight: bold; /* Làm cho chữ đậm */
}

QTabBar::tab:selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #1abc9c, stop:1 #16a085); /* Gradient nền của tab khi chọn */
    border-color: #1abc9c; /* Viền của tab khi chọn */
}

QTabBar::tab:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #16a085, stop:1 #1abc9c); /* Gradient nền của tab khi hover */
}

/* Tùy chỉnh cho các nút */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #3498db, stop:1 #2980b9); /* Gradient nền của nút */
    color: white; /* Màu chữ */
    border: 2px solid #2980b9; /* Viền của nút */
    border-radius: 15px; /* Bo tròn góc của nút */
    padding: 15px 30px; /* Khoảng cách giữa văn bản và viền */
    font-size: 16px; /* Kích thước font chữ */
    font-weight: bold; /* Làm cho chữ đậm */
    transition: background-color 0.3s, border-color 0.3s, color 0.3s; /* Hiệu ứng chuyển màu */
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #1abc9c, stop:1 #16a085); /* Gradient nền của nút khi hover */
    color: white; /* Màu chữ khi hover */
    border-color: #16a085; /* Viền của nút khi hover */
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #16a085, stop:1 #1abc9c); /* Gradient nền của nút khi nhấn */
    border-color: #1abc9c; /* Viền của nút khi nhấn */
    color: white; /* Màu chữ khi nhấn */
    transform: scale(0.98); /* Tạo hiệu ứng co lại khi nhấn */
}

/* Tùy chỉnh cho các tab khi không chọn */
QTabBar::tab:!selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #34495e, stop:1 #2c3e50); /* Gradient nền cho tab chưa chọn */
    border-color: #2c3e50; /* Viền cho tab chưa chọn */
    color: #ecf0f1; /* Màu chữ cho tab chưa chọn */
}

/* Đảm bảo rằng tab có hiệu ứng khi hover */
QTabBar::tab:hover:!selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #1abc9c, stop:1 #16a085); /* Gradient nền khi hover vào tab chưa chọn */
    border-color: #16a085;
}

